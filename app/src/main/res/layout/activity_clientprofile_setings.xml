<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_clientprofile_setings"
    android:theme="@android:style/Theme.Holo.NoActionBar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bennitai.ClientprofileSetings">


    <android.support.v7.widget.Toolbar
        android:id="@+id/my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:background="@color/colorPrimary"
        android:gravity="center">

    </android.support.v7.widget.Toolbar>

    <TextView
        android:text="name"
        android:textColor="#000000"
        android:gravity="center_vertical"
        android:textAlignment="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView4"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="10dp"
        android:id="@+id/name_view" />

    <TextView
        android:text="email"
        android:textColor="#000000"
        android:textAlignment="center"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/name_view"
        android:layout_marginTop="0dp"
        android:id="@+id/email_view"
        android:layout_alignRight="@+id/name_view"
        android:layout_alignEnd="@+id/name_view" />

    <Button
        android:text="Disconnected"
         android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:layout_alignBottom="@+id/my_toolbar"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_delete"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/Fromclient_goback" />


    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/my_toolbar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/scrollView">
    <RelativeLayout
        android:layout_below="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="400dp">
    <TextView
        android:text="PERSONAL DETAILS"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_below="@+id/my_toolbar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="11dp" />

    <TextView
        android:text="Edit"
        android:textColor="@color/colorPrimary"
        android:textSize="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/client_profile_edit"
        android:layout_marginRight="22dp"
        android:layout_alignRight="@+id/textView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

         <TextView
            android:text="Employer"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/employer_text_caption"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView" />

        <TextView
            android:text="Role"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/Role_text_caption"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/employer_text_caption" />

        <TextView
            android:text="Role value"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:id="@+id/Role_text_value"
            android:layout_below="@+id/employer_text_value"
            android:layout_marginRight="15dp"
            android:layout_toLeftOf="@+id/client_profile_edit" />

        <TextView
            android:text="empl value"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/employer_text_value"
            android:layout_alignBaseline="@+id/employer_text_caption"
            android:layout_alignBottom="@+id/employer_text_caption"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="15dp"
            android:layout_marginEnd="18dp"
            android:layout_alignLeft="@+id/Role_text_value"
            android:layout_alignStart="@+id/Role_text_value" />

        <TextView
            android:text="Area"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/Area_text_caption"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/Role_text_caption" />

        <TextView
            android:text="Area value"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:id="@+id/Area_text_value"
            android:layout_below="@+id/Role_text_value"
            android:layout_marginRight="15dp"
            android:layout_toLeftOf="@+id/client_profile_edit" />

       <TextView
        android:text="Interests"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:id="@+id/Interests_text_caption"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Area_text_caption" />

        <TextView
            android:text="Goal"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/Goal_text_caption"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/Interests_text_caption" />

        <TextView
            android:text="Interests"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:id="@+id/Interests_text_value"
            android:layout_below="@+id/Area_text_value"
            android:layout_marginRight="15dp"
            android:layout_toLeftOf="@+id/client_profile_edit" />

        <TextView
            android:text="Goal"
            android:textSize="15dp"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:id="@+id/Goal_text_value"
            android:layout_below="@+id/Interests_text_value"
            android:layout_marginRight="15dp"
            android:layout_toLeftOf="@+id/client_profile_edit" />

        <TextView
            android:text="Enable Text to Speech"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/enable_t2s_title"
            android:layout_marginTop="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView3" />

        <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/enable_t2s_switch"
            android:layout_alignTop="@+id/enable_t2s_title"
            android:layout_below="@+id/enable_t2s_switch"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <TextView
            android:text="Enable Start of day\n notification"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/enable_startdaynot_title"
            android:layout_marginTop="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/enable_t2s_title" />

        <TextView
            android:text="Enable end of day\n notification"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/enable_eOd_title"
            android:layout_marginTop="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/enable_startdaynot_title" />

        <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/enable_eOd_switch"
            android:layout_alignTop="@+id/enable_eOd_title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/enable_sOd_switch"
            android:layout_alignTop="@+id/enable_startdaynot_title"
            android:layout_toRightOf="@+id/textView"
            android:layout_toEndOf="@+id/textView" />

        <TextView
            android:text="Track Goal"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/trackGoaltext"
            android:layout_marginTop="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/enable_eOd_title" />

        <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/trackGoal_switch"
            android:layout_below="@+id/enable_eOd_title"
            android:layout_alignLeft="@+id/enable_sOd_switch"
            android:layout_alignStart="@+id/enable_sOd_switch"
            android:layout_marginLeft="9dp"
            android:layout_marginStart="9dp" />

        <TextView
            android:text="CUSTOMIZE YOUR EXPERIENCE"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:layout_marginTop="28dp"
            android:layout_below="@+id/Goal_text_caption"
            android:layout_alignLeft="@+id/textView"
            android:layout_alignStart="@+id/textView" />

        <TextClock
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="21dp"
            android:layout_marginEnd="21dp"
            android:id="@+id/SODtextClock"
            android:layout_above="@+id/enable_eOd_title"
            android:layout_toLeftOf="@+id/Goal_text_value"
            android:layout_toStartOf="@+id/Goal_text_value"
            android:layout_marginBottom="7dp" />
        <TextClock
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="21dp"
            android:layout_marginEnd="21dp"
            android:id="@+id/EODtextClock"
            android:layout_above="@+id/trackGoal_switch"
            android:layout_toLeftOf="@+id/Goal_text_value"
            android:layout_toStartOf="@+id/Goal_text_value"
            android:layout_marginBottom="7dp" />

        <TextClock
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="21dp"
            android:layout_marginEnd="21dp"
            android:id="@+id/PNtextClock"
            android:layout_above="@+id/ProdNot_switch"
            android:layout_toLeftOf="@+id/Goal_text_value"
            android:layout_toStartOf="@+id/Goal_text_value"
            android:layout_marginBottom="7dp" />

        <TextView
            android:text="Enable Productivity\n Notifications"
            android:textColor="#000000"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:id="@+id/ProdNotificationtitle"
            android:layout_marginTop="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/trackGoaltext" />

        <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ProdNot_switch"
            android:layout_below="@+id/trackGoal_switch"
            android:layout_alignLeft="@+id/enable_sOd_switch"
            android:layout_alignStart="@+id/enable_sOd_switch"
            android:layout_marginLeft="9dp"
            android:layout_marginStart="9dp" />


    </RelativeLayout>
    </ScrollView>

    <ImageView
      android:layout_width="80dp"
      android:layout_height="80dp"
      android:src="@drawable/rounded_corners"
        android:id="@+id/imageView4"
        android:layout_below="@+id/Fromclient_goback"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />


</RelativeLayout>